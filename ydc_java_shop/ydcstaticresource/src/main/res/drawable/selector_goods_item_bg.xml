<?xml version="1.0" encoding="utf-8"?><!--商品列表item的背景色-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/white_down" />

            <stroke android:width="@dimen/line_1_px" android:color="#eeeeee" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/white" />

            <stroke android:width="@dimen/line_1_px" android:color="#eeeeee" />
        </shape>
    </item>
</selector>

