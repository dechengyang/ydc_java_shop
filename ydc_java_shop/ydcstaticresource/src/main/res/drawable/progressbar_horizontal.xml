<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
        <clip>
            <shape>
                <corners android:radius="0dip" />
                <gradient android:startColor="#9d9d9c" android:endColor="#9d9d9c" android:angle="270" android:centerY="0.75"/>
            </shape>
        </clip>
    </item>
	<item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0dip" />
                <gradient android:startColor="#9d9d9c" android:endColor="#9d9d9c" android:angle="270" android:centerY="0.75"/>
            </shape>
        </clip>
    </item>
    
	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<gradient android:startColor="#9d9d9c" android:endColor="#9d9d9c" android:angle="270" android:centerY="0.25"/>
				<corners android:radius="0dip"/>
			</shape>
		</clip>
	</item>
</layer-list>